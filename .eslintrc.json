{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "react-app",
        "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prettier/prettier": [1,{
            "semi": false,
            "singleQuote": true,
            "tabWidth": 2,
            "useTabs": false,
            "endOfLine": "auto"
        }],
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": 0,
        "react/no-children-prop": 0,
        "@typescript-eslint": 0
    }
}